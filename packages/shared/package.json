{
  "name": "@bookmarks/shared",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "pnpm build --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "vitest run --passWithNoTests",
    "prepare": "pnpm build"
  },
  "exports": {
    ".": {
