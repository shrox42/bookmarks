{
  "name": "bookmarks-monorepo",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r build && SKIP_EXTENSION_BUILD=1 pnpm package-extension",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "format": "pnpm -r format",
    "package-extension": "./scripts/package-extension.sh"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "bestzip": "^2.2.1",
    "globals": "^15.12.0",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "typescript-eslint": "^8.19.1"
  }
}
